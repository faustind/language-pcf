{- List -}
let rec indx xs n 
  = if iszero n 
      then head xs 
      else (indx (tail xs) (n-1));
let rec map f xs
  = if null xs
      then []
      else cons (f (head xs)) (map f (tail xs));

{- Pairs. -}

let Pair x y = [x, y];
let fst  p = head p;
let snd  p = head (tail p);

{- Logic -}
let not p = if p then false else true;

